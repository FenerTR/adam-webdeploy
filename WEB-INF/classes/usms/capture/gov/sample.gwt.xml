<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="sample">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Added by Adam per GXT requirement -->
    <!--Other module inherits -->
    <inherits name="com.sencha.gxt.ui.GXT" />
    <inherits name="com.sencha.gxt.chart.Chart" />
    <!--inherits name="com.documentum.fc.client.IDfDocument"/-->

    <!-- GXT Stylesheet-->
    <stylesheet src="reset.css" />

    <!-- Neptune Theme -->
    <inherits name="com.sencha.gxt.theme.neptune.Theme" />

    <!-- ADAM NOTE: USE GOOGLE CHROME FOR YOUR BROWSER. IE DOES NOT REFLECT CHANGES -->
    <!-- Specify the app entry point class.-->
    <!-- entry-point class='sample'/ -->
    <!--entry-point class="usms.capture.gov.client.SamplePieGraph"/-->
    <!--entry-point class="usms.capture.gov.client.SampleForm"/-->
    <entry-point class="usms.capture.gov.client.FileUpload"/>

    <!-- Specify the app servlets.                   -->
    <servlet path='/sampleService' class='usms.capture.gov.client.sampleServiceImpl'/>

    <!--Specify the paths for translatable code -->
    <source path="client" />
    <source path="shared" />

</module>